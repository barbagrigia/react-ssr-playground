{
  "name": "with-redux",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run sass-watch & npm run next-dev",
    "build": "npm run sass && next build",
    "sass": "node-sass --recursive --source-map=true --output-style=compressed --output ./static ./static",
    "sass-watch": "npm run sass && npm run sass -- --watch --quiet --output-style=nested",
    "next-dev": "next",
    "next-server": "next start"
  },
  "dependencies": {
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "next": "^2.0.0-beta",
    "next-redux-wrapper": "^1.0.0",
    "node-sass": "^4.5.0"
  }
}
